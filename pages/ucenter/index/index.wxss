}

.user-menu .icon.feedback{
    background: url(data:image/png;base64,) 0 0 no-repeat;
    background-size: 52rpx;
}
.user-menu .icon.phone{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 52rpx;
}


.user-menu .txt{
    display: block;
    height: 24rpx;
    width: 100%;
    font-size: 24rpx;
    color:#333;
}

.logout{
    margin-top: 50rpx;
    height: 101rpx;
    width: 100%;
    line-height: 101rpx;
    text-align: center;
    background: #fff;
    color: #333;
    font-size: 30rpx;
}

.service {
  position: static;
  background-color: transparent;
  color: transparent;
  margin: 0;
  padding: 0;
  border: none;
  text-align: left;
  line-height: normal;
  display: inline;
}

.company {
  font-size:20rpx;
  text-align:center;
  margin-top:50px;
}